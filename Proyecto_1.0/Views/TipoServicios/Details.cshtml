@model Proyecto_1._0.Models.TipoServicio

@{
    ViewBag.Title = "Details";
}

<h2>Mostrar Detalles</h2>

<div>
    <h4>Servicios</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Nombre de la institución")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Instituciones.nombre_institucion)
        </dd>

        <dt>
            @Html.DisplayName("Nombre del servicio")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre_servicio)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id_servicio }) |
    @Html.ActionLink("Volver", "Index")
</p>
