@model Proyecto_1._0.Models.RequisitoServicioTramite

@{
    ViewBag.Title = "Details";
}

<h2>Mostrar Detalles</h2>

<div>
    <h4>Requisito Servicio-Trámite</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Nombre del servicio")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TipoServicio.nombre_servicio)
        </dd>

        <dt>
            @Html.DisplayName("Nombre del trámite")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TipoTramite.nombre_tramite)
        </dd>

        <dt>
            @Html.DisplayName("Obligatorio")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.es_obligatorio)
