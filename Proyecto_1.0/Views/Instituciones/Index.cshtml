@model IEnumerable<Proyecto_1._0.Models.Instituciones>

@{
    ViewBag.Title = "Index";
}

<h2>Catálogo de instituciones</h2>

<p>
    @Html.ActionLink("Registrar Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.ActionLink("Nombre de la institución", "Index", new { sortOrder = ViewBag.NameSortParm })
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre_institucion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id_institucion }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.id_institucion }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id_institucion })
        </td>
    </tr>
}

</table>
