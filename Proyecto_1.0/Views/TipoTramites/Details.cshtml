@model Proyecto_1._0.Models.TipoTramite

@{
    ViewBag.Title = "Details";
}

<h2>Mostrar Detalles</h2>

<div>
    <h4>Tramites</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Nombre de la institución")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Instituciones.nombre_institucion)
        </dd>

        <dt>
            @Html.DisplayName("Nombre del trámite")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre_tramite)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id_tramite }) |
    @Html.ActionLink("Volver", "Index")
</p>
